<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Autopark{% endblock %}</title>

    {% block stylesheets %}
        <link  href="{{ asset('assets/css/sign-up.css') }}" rel="stylesheet" />
    {% endblock %}

    {% block javascripts %}

    {% endblock %}
</head>
<body>
{% block body %}

    <div>

        <img id="" src={{ asset('assets/images/parking.png') }} alt="" >

    </div>

    <form action="" method="Post">
        {{ form_widget(formClient) }}
        <input type="submit" value="Reserver">
    </form>

    <div class="box">
        <div class="form">
            <h2>Sign Up</h2>
            <div class="inputBox">
                <input type="text" required="required">
                <span>Username</span>
                <i></i>
            </div>
            <div class="inputBox">
                <input type="date" required="required">
                <span></span>
                <i></i>
            </div>
            <div class="inputBox">
                <input type="text" required="required">
                <span>Lisence Plate of the vehicle</span>
                <i></i>
            </div>
            <div class="inputBox">
                <input type="password" required="required">
                <span>Password</span>
                <i></i>
            </div>
            <div class="inputBox">
                <input type="password" required="required">
                <span>Retype Password</span>
                <i></i>
            </div>
            <div class="links">
                <br>
                <br>      <br><br>

                <a href="{{ path('app_login') }}">Go to Log In</a>
            </div>


            <input type="submit" value="SignUp" class="c">
        </div>
    </div>




{% endblock %}
</body>
</html>
