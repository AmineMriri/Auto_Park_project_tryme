<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Autopark{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}


         <link href="{{ asset('assets/css/login.css') }}" rel="stylesheet"/>


    {% endblock %}

    {% block javascripts %}
        {#{{ encore_entry_script_tags('app') }}#}
    {% endblock %}
</head>
<body>
{% block body %}
    <div><img id="photo" src={{ asset('assets/images/parking.png')}} alt="" ></div>
    <div class="box">
        <div class="form">
            <h2>Sign In</h2>
            <div class="inputBox">
                <input type="text" required="required">
                <span>Username</span>
                <i></i>
            </div>
            <div class="inputBox">
                <input type="password" required="required">
                <span>Password</span>
                <i></i>
            </div>
            <div class="links">
                <br>
                <br>      <br><br>      <a href="#">Forgot Password?</a>

                <a href="{{ path('app_inscrit') }}">Sign Up</a>
            </div>


            <input type="submit" value="Login" class="c">
        </div>
    </div>

{% endblock %}
</body>
</html>
